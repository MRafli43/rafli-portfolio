---
import { getEntry, render } from "astro:content";

const entry = await getEntry("experience", "index");

if (!entry) {
  throw new Error("Experience content not found");
}

const { Content } = await render(entry);
---

<main>
  <h1>{entry.data.title}</h1>
  <article>
    <Content />
  </article>
</main>